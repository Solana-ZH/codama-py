{
  "name": "copyma",
  "version": "0.1.1-beta",
  "description": "Python renderer compatible with the Solana Kit library",
  "main": "./dist/index.node.cjs",
  "module": "./dist/index.node.mjs",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && pnpm build:src && pnpm build:types",
    "build:src": "tsup src/index.ts --format cjs,esm --no-dts --platform node --config tsup.config.node.ts",
    "build:types": "tsc --build tsconfig.declarations.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "NODE_ENV=test vitest run --config test/config/vitest.config.ts",
    "test:e2e": "sh ./e2e/test.sh",
    "genpy": "tsx ./bin/genpy.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "dependencies": {
    "@codama/errors": "^1.3.3",
    "@codama/nodes": "^1.3.3",
    "@codama/nodes-from-anchor": "^1.2.5",
    "@codama/renderers-core": "^1.0.19",
    "@codama/visitors-core": "^1.3.3",
    "codama": "^1.3.3",
    "commander": "^14.0.0",
    "nunjucks": "^3.2.4",
    "tsx": "^4.20.4"
  },
  "devDependencies": {
    "@types/node": "24.3.0",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^9.33.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daog1/copyma.git"
  },
  "bugs": {
    "url": "https://github.com/daog1/copyma/issues"
  },
  "browserslist": [
    "supports bigint and not dead",
    "maintained node versions"
  ]
}
